<div class="col-sm-6">
<h1>Check out entries for...</h1>
<br/>


  <ul class="list-group">
  	<%= link_to params[:location], '#', class: "list-group-item active" %>
    <% @articles.each do |article| %>
      <div id="article_id-<%= article.id %>">
        <li class="list-group-item"><%= link_to article.title, article %>
        
        </li>
      </div>      
    <% end %>
  </ul>
</div>